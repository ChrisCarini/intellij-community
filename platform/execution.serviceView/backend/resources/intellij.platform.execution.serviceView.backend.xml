<idea-plugin>
  <dependencies>
    <module name="intellij.platform.backend"/>
    <!-- region Generated dependencies - run `Generate Product Layouts` to regenerate -->
    <module name="intellij.platform.core"/>
    <module name="intellij.platform.core.impl"/>
    <module name="intellij.platform.execution.serviceView"/>
    <module name="intellij.platform.kernel.backend"/>
    <!-- endregion -->
  </dependencies>

  <extensionPoints>
    <extensionPoint name="serviceViewLocatableSearcher" interface="com.intellij.platform.execution.serviceView.backend.ServiceViewLocatableSearcher" dynamic="true"/>
  </extensionPoints>

  <extensions defaultExtensionNs="com.intellij">
    <platform.rpc.backend.remoteApiProvider implementation="com.intellij.platform.execution.serviceView.backend.BackendServiceViewRpcApiProvider"/>
  </extensions>
</idea-plugin>
