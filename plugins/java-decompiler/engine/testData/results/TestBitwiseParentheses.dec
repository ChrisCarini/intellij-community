package pkg;

public class TestBitwiseParentheses {
   public static void main(String[] args) {
      System.out.println(bitwiseAndWithAdd(255, 3, 5));// 6
      System.out.println(bitwiseOrWithMul(15, 3, 5));// 7
      System.out.println(bitwiseXorWithSub(255, 10, 3));// 8
      System.out.println(shiftRightWithSub(65280, 8, 4));// 9
      System.out.println(shiftLeftWithAdd(1, 2, 3));// 10
      System.out.println(unsignedShiftWithMul(-1, 2, 4));// 11
      System.out.println(addInsideBitwiseLeft(3, 5, 255));// 12
      System.out.println(mulInsideShiftLeft(3, 2, 4));// 13
      System.out.println(complexMixed(255, 65280, 8, 4));// 14
      System.out.println(pureArithmetic(10, 3, 5));// 16
      System.out.println(pureBitwiseSame(255, 15, 240));// 17
      System.out.println(pureBitwiseDifferentPrec(255, 15, 1));// 18
      System.out.println(bitwiseChildInsideArithmetic(15, 3, 10));// 19
      System.out.println(shiftChildInsideArithmetic(8, 2, 5));// 20
   }// 21

   static int bitwiseAndWithAdd(int a, int b, int c) {
      return a & (b + c);// 24
   }

   static int bitwiseOrWithMul(int a, int b, int c) {
      return a | (b * c);// 28
   }

   static int bitwiseXorWithSub(int a, int b, int c) {
      return a ^ (b - c);// 32
   }

   static int shiftRightWithSub(int a, int b, int c) {
      return a >> (b - c);// 36
   }

   static int shiftLeftWithAdd(int a, int b, int c) {
      return a << (b + c);// 40
   }

   static int unsignedShiftWithMul(int a, int b, int c) {
      return a >>> (b * c);// 44
   }

   static int addInsideBitwiseLeft(int a, int b, int c) {
      return (a + b) & c;// 48
   }

   static int mulInsideShiftLeft(int a, int b, int c) {
      return (a * b) << c;// 52
   }

   static int complexMixed(int mask, int value, int total, int bits) {
      return mask & value >> (total - bits);// 56
   }

   static int pureArithmetic(int a, int b, int c) {
      return a + b * c;// 60
   }

   static int pureBitwiseSame(int a, int b, int c) {
      return a & b & c;// 64
   }

   static int pureBitwiseDifferentPrec(int a, int b, int c) {
      return a | b & c;// 68
   }

   static int bitwiseChildInsideArithmetic(int a, int b, int c) {
      return (a & b) + c;// 72
   }

   static int shiftChildInsideArithmetic(int a, int b, int c) {
      return (a << b) + c;// 76
   }
}

